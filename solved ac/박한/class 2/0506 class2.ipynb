   "source": [
    "# 백준 class 2++ 1259 팰린드롬수\n",
    "\n",
    "while 1:\n",
    "    n = input()\n",
    "    if n == '0':\n",
    "        break\n",
    "    rst = 0\n",
    "    for i in range(len(n)//2):\n",
    "        if n[i] != n[len(n)-i-1]:\n",
    "            print(\"no\")\n",
    "            break\n",
    "        else:\n",
    "            rst += 1\n",
    "    if rst == len(n)//2:\n",
    "        print(\"yes\")"
   ]
  }
