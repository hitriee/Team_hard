    "# 백준 class 2++ 2839 설탕 배달\n",
    "\n",
    "n = int(input())\n",
    "rst = 0\n",
    "while 1:\n",
    "    if n%5 == 0:\n",
    "        rst += n//5\n",
    "        print(rst)\n",
    "        break\n",
    "    else:\n",
    "        n -= 3\n",
    "        rst += 1\n",
    "    if n < 0:\n",
    "        print(-1)\n",
    "        break"
   ]
  }
