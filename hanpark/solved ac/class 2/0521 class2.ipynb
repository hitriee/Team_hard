   "source": [
    "# 백준 class 2++ 2805 나무 자르기\n",
    "\n",
    "n, m = map(int, input().split())\n",
    "lst = list(map(int, input().split()))\n",
    "rst = max(lst)-1\n",
    "while 1:\n",
    "    ans = 0\n",
    "    for i in range(n):\n",
    "        if lst[i] > rst:\n",
    "            ans += lst[i]-rst\n",
    "    if ans >= m:\n",
    "        print(rst)\n",
    "        break\n",
    "    rst -= 1"
   ]
  }
