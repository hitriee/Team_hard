   "source": [
    "# 백준 class 2++ 4153 직각삼각형\n",
    "\n",
    "while 1:\n",
    "    lst = list(map(int, input().split()))\n",
    "    if sum(lst) == 0:\n",
    "        break\n",
    "    a = max(lst)\n",
    "    b = 0\n",
    "    for i in range(3):\n",
    "        b += (lst[i])**2\n",
    "    if (a**2)*2 == b:\n",
    "        print(\"right\")\n",
    "    else:\n",
    "        print(\"wrong\")"
   ]
  }
